			{
				"role":"container",
				"contains":[
					"instanceMeasure",
					"processMeasure",
					"aggregatedMeasureGeneric"
				]
			}
		],
		"morphingRules": [
			{
				"role": "baseMeasureMorph",
				"baseMorphs": ["timeMeasure"] 
			},
			{
				"role": "aggregatedMeasure",
				"baseMorphs": ["timeAggregatedMeasure"] 
			},
			{
