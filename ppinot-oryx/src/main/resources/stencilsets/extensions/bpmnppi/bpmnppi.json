				"baseMorphs": ["derivedSingleInstanceMeasure"] 
			},
			
			{
				"role": "ppiMorph",
				"baseMorphs": ["ppi"]
			},
			
			{
				"role": "container",
				"baseMorphs": ["container"]
			},
			
			{
				"role": "ConnectorsMorph",
				"baseMorphs": ["timeConnector"] 
			}
		]
	}
